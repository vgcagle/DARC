    </owl:ObjectProperty>
    


    <!-- http://www.cirma.unito.it/drammar/drammar.owl#knows -->

    <owl:ObjectProperty rdf:about="http://www.cirma.unito.it/drammar/drammar.owl#knows">
        <rdfs:subPropertyOf rdf:resource="http://www.cirma.unito.it/drammar/drammar.owl#hasMentalState"/>
