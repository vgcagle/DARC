        <rdfs:comment>Property of an agent, who BELIEVES a Belief (a mental state with a propositional content).
BELIEVES = KNOWS.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.cirma.unito.it/drammar/drammar.owl#love_appraisingAgent -->

    <owl:ObjectProperty rdf:about="http://www.cirma.unito.it/drammar/drammar.owl#love_appraisingAgent">
        <rdfs:subPropertyOf rdf:resource="http://www.cirma.unito.it/drammar/drammar.owl#appraisingAgent"/>
        <rdfs:domain rdf:resource="http://www.cirma.unito.it/drammar/drammar.owl#Emotion"/>
        <rdfs:range rdf:resource="http://www.cirma.unito.it/drammar/drammar.owl#Agent"/>
    </owl:ObjectProperty>
    


    <!-- http://www.cirma.unito.it/drammar/drammar.owl#love_target -->

    <owl:ObjectProperty rdf:about="http://www.cirma.unito.it/drammar/drammar.owl#love_target">
        <rdfs:subPropertyOf rdf:resource="http://www.cirma.unito.it/drammar/drammar.owl#target"/>
        <rdfs:domain rdf:resource="http://www.cirma.unito.it/drammar/drammar.owl#Emotion"/>
        <rdfs:range rdf:resource="http://www.cirma.unito.it/drammar/drammar.owl#Agent"/>
    </owl:ObjectProperty>
    


    <!-- http://www.cirma.unito.it/drammar/drammar.owl#precedes -->

    <owl:ObjectProperty rdf:about="http://www.cirma.unito.it/drammar/drammar.owl#precedes">
        <rdfs:domain rdf:resource="http://www.cirma.unito.it/drammar/drammar.owl#OrderedListElement"/>
        <rdfs:range rdf:resource="http://www.cirma.unito.it/drammar/drammar.owl#OrderedListElement"/>
        <rdfs:comment>Sequential order in an ordered list.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.cirma.unito.it/drammar/drammar.owl#propositionalContent -->

    <owl:ObjectProperty rdf:about="http://www.cirma.unito.it/drammar/drammar.owl#propositionalContent">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.cirma.unito.it/drammar/drammar.owl#BeliefSchema"/>
                    <rdf:Description rdf:about="http://www.cirma.unito.it/drammar/drammar.owl#GoalSchema"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:comment>propositionalContent links  BeliefSchema or a GoalSchema) to the its content (a factual state or process), that is the Process or State that is the actual content of the belief or the goal.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.cirma.unito.it/drammar/drammar.owl#shame_appraisingAgent -->

    <owl:ObjectProperty rdf:about="http://www.cirma.unito.it/drammar/drammar.owl#shame_appraisingAgent">
        <rdfs:subPropertyOf rdf:resource="http://www.cirma.unito.it/drammar/drammar.owl#appraisingAgent"/>
        <rdfs:domain rdf:resource="http://www.cirma.unito.it/drammar/drammar.owl#Emotion"/>
        <rdfs:range rdf:resource="http://www.cirma.unito.it/drammar/drammar.owl#Agent"/>
    </owl:ObjectProperty>
    


    <!-- http://www.cirma.unito.it/drammar/drammar.owl#shame_target -->

    <owl:ObjectProperty rdf:about="http://www.cirma.unito.it/drammar/drammar.owl#shame_target">
        <rdfs:subPropertyOf rdf:resource="http://www.cirma.unito.it/drammar/drammar.owl#target"/>
        <rdfs:domain rdf:resource="http://www.cirma.unito.it/drammar/drammar.owl#Emotion"/>
        <rdfs:range rdf:resource="http://www.cirma.unito.it/drammar/drammar.owl#Agent"/>
    </owl:ObjectProperty>
    


    <!-- http://www.cirma.unito.it/drammar/drammar.owl#spans -->

    <owl:ObjectProperty rdf:about="http://www.cirma.unito.it/drammar/drammar.owl#spans">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.cirma.unito.it/drammar/drammar.owl#Scene"/>
        <rdfs:range rdf:resource="http://www.cirma.unito.it/drammar/drammar.owl#Timeline"/>
        <rdfs:comment>The coincidence relation of a Scene on a Timeline.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/2002/07/owl#topObjectProperty -->

    <rdf:Description rdf:about="http://www.w3.org/2002/07/owl#topObjectProperty">
